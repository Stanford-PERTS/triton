{
  "name": "triton",
  "description": "PERTS Improvement Challenge Platform",
  "version": "0.0.1",
  "engines": {
    "node": "10"
  },
  "license": "UNLICENSED",
  "repository": {
    "type": "git",
    "url": "git://github.com/PERTS/triton.git"
  },
  "scripts": {
    "cleandeps": "rm -rf node_modules",
    "postinstall": "cp git_hooks/* .git/hooks/ && chmod +x .git/hooks/*",
    "server": "dev_appserver.py . -A=triton-dev --port=10080 --admin_port=10008 --storage_path=.gae_sdk --enable_console=yes --enable_host_checking=no --support_datastore_emulator=true",
    "server:mysql": "mysql.server start",
    "start": "BROWSER=none react-scripts start",
    "start:open": "react-scripts start",
    "build": "react-scripts build",
    "postbuild": "cp build/index.html templates/index.html",
    "test": "react-scripts test --env=jsdom --verbose=false",
    "test:CI": "react-scripts test --env=jsdom",
    "test:pre-push": "NODE_PATH=src CI=true cypress run --config video=false,baseUrl=http://localhost:10080",
    "test:cypress": "NODE_PATH=src cypress open",
    "faker": "NODE_PATH=src babel-node scripts/generateTestData.js --presets @babel/env",
    "eslint": "eslint src"
  },
  "dependencies": {
    "@babel/node": "7.0.0",
    "@babel/preset-env": "7.1.0",
    "@babel/runtime": "7.3.1",
    "antd": "3.22.1",
    "babel-eslint": "10.0.3",
    "capitalize": "2.0.0",
    "chart.js": "2.7.3",
    "classnames": "2.2.5",
    "core-js": "2.5.7",
    "deep-freeze": "0.0.1",
    "enzyme": "3.7.0",
    "enzyme-adapter-react-16": "1.12.1",
    "escape-string-regexp": "2.0.0",
    "eslint": "6.5.1",
    "eslint-config-prettier": "6.4.0",
    "eslint-plugin-babel": "5.3.0",
    "eslint-plugin-prettier": "3.1.1",
    "eslint-plugin-react": "7.16.0",
    "faker": "4.1.0",
    "fetch-dedupe": "3.0.0",
    "font-awesome": "4.7.0",
    "html-react-parser": "0.4.7",
    "http-link-header": "0.8.0",
    "jsonwebtoken": "8.4.0",
    "left-pad": "1.3.0",
    "lodash": "4.17.15",
    "moment": "2.22.2",
    "pluralize": "7.0.0",
    "prettier": "1.18.2",
    "prop-types": "15.6.0",
    "react": "16.8.4",
    "react-chartjs-2": "2.7.0",
    "react-collapse": "4.0.3",
    "react-dom": "16.8.4",
    "react-dropzone": "4.0.0",
    "react-fake-props": "0.3.2",
    "react-media": "1.8.0",
    "react-motion": "0.5.2",
    "react-phone-number-input": "0.12.7",
    "react-redux": "5.0.4",
    "react-router-dom": "5.1.2",
    "react-scripts": "3.3.0",
    "react-simple-dropdown": "3.2.3",
    "react-table": "7.0.4",
    "react-transition-group": "1.1.3",
    "recompose": "0.30.0",
    "redux": "4.0.3",
    "redux-form": "7.4.2",
    "redux-form-saga": "0.2.0",
    "redux-mock-store": "1.2.3",
    "redux-saga": "0.15.3",
    "redux-saga-test-plan": "3.3.0",
    "reselect": "3.0.1",
    "styled-components": "4.3.2",
    "stylis-rule-sheet": "0.0.10",
    "typescript": "3.5.3",
    "unfetch": "4.0.1",
    "urijs": "1.18.12",
    "uuid": "3.1.0",
    "validator": "7.0.0"
  },
  "devDependencies": {
    "@types/jest": "^24.0.15",
    "@types/lodash": "^4.14.138",
    "@types/node": "^12.6.2",
    "@types/react": "^16.8.23",
    "@types/react-dom": "^16.8.4",
    "@types/react-redux": "^7.1.1",
    "@types/react-router-dom": "^4.3.5",
    "@types/recompose": "^0.30.7",
    "@types/redux-form": "^8.1.4",
    "@types/styled-components": "^4.1.18",
    "chalk": "2.1.0",
    "cypress": "3.4.1",
    "db-migrate": "^0.11.11",
    "db-migrate-mysql": "^2.1.1",
    "js-yaml": "3.13.1",
    "mysql": "2.16.0"
  },
  "proxy": "http://localhost:10080",
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "babelMacros": {
    "styledComponents": {
      "pure": true
    }
  }
}
